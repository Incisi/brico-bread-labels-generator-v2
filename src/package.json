{
  "name": "Brico Bread - Gerador de Etiquetas",
  "version": "2.0.0",
  "main": "index.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder"
  },
  "build": {
    "appId": "br.dev.incisi",
    "productName": "Brico Bread - Gerador de Etiquetas",
    "directories": {
      "output": "brico-bread-generator-labels-application"
    },
    "files": [
      "index.js",
      "js/**/*",
      "data/**/*",
      "config.json",
      "css/**/*",
      "media/**/*",
      "fonts/**/*",
      "index.html",
      "node_modules/**/*"
    ],
    "win": {
      "target": "nsis"
    },
    "mac": {
      "target": "dmg"
    },
    "linux": {
      "target": "AppImage"
    }
  },
  "keywords": [],
  "author": "Incisi",
  "license": "ISC",
  "description": "Gerador de etiquetas multi-loja com backup e lixeira.",
  "dependencies": {
    "pdfkit": "^0.15.1",
    "sweetalert2": "^11.14.4"
  },
  "devDependencies": {
    "electron": "^33.0.2",
    "electron-builder": "^25.1.8"
  }
}